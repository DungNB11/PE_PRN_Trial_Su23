@{
}
@using Q3.Models;
@addTagHelper *,Microsoft.AspNetCore.Mvc.TagHelpers
<h1>List of Employees</h1>
<table>
    <tr>
        <td>Name</td>
        <td>Sex</td>
        <td>Position</td>
        <td></td>
    </tr>
    @foreach (Employee s in Model)
    {
        <tr>
            
            <td>@s.Name</td>
            <td>@s.Sex</td>
            <td>@s.Position</td>
            <td><a a asp-controller="Employee" asp-action="Delete" asp-route-Id="@s.Id"> Delete</a></td>
        </tr>
    }
</table>